{
  "name": "@nest-template/graphql",
  "version": "0.0.1",
  "description": "Nest.js GraphQL",
  "main": "dist/main",
  "repository": "git@github.com:hieudoanm/nest-graphql-template.git",
  "author": "hieudoanm <hieumdoan@gmail.com>",
  "license": "GNU GPL 3.0",
  "private": true,
  "engines": {
    "node": "20.1.0",
    "npm": "9.6.7",
    "yarn": "1.22.19"
  },
  "scripts": {
    "prebuild": "rimraf dist",
    "build": "nest build",
    "postbuild": "yarn docs:postman && yarn docs:html",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "start": "nest start",
    "start:dev": "kill-port 5000 && nest start --watch",
    "start:debug": "kill-port 5000 && nest start --debug --watch",
    "start:prod": "kill-port 5000 && node dist/main",
    "docs:postman": "openapi2postmanv2 -s docs/swagger/swagger.yaml -o docs/postman/collection.json",
    "docs:html": "redocly build-docs ./docs/swagger/swagger.json --output ./public/index.html --title 'Nest.js - API - Template'",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "jest --config ./test/jest-e2e.json"
  },
  "dependencies": {
    "@apollo/server": "4.7.1",
    "@nestjs/apollo": "11.0.5",
    "@nestjs/common": "9.4.2",
    "@nestjs/core": "9.4.2",
    "@nestjs/graphql": "11.0.5",
    "@nestjs/platform-express": "9.4.2",
    "@nestjs/swagger": "6.3.0",
    "graphql": "16.6.0",
    "helmet": "7.0.0",
    "js-yaml": "4.1.0",
    "reflect-metadata": "0.1.13",
    "rxjs": "7.8.1"
  },
  "devDependencies": {
    "@nestjs/cli": "9.5.0",
    "@nestjs/schematics": "9.2.0",
    "@nestjs/testing": "9.4.2",
    "@redocly/cli": "1.0.0-beta.126",
    "@types/express": "4.17.17",
    "@types/jest": "29.5.1",
    "@types/node": "20.2.3",
    "@types/supertest": "2.0.12",
    "@typescript-eslint/eslint-plugin": "5.59.7",
    "@typescript-eslint/parser": "5.59.7",
    "eslint": "8.41.0",
    "eslint-config-prettier": "8.8.0",
    "eslint-plugin-prettier": "4.2.1",
    "jest": "29.5.0",
    "kill-port": "2.0.1",
    "openapi-to-postmanv2": "4.13.0",
    "prettier": "2.8.8",
    "pretty-quick": "3.1.3",
    "rimraf": "5.0.1",
    "source-map-support": "0.5.21",
    "spectaql": "2.0.9",
    "supertest": "6.3.3",
    "ts-jest": "29.1.0",
    "ts-loader": "9.4.3",
    "ts-node": "10.9.1",
    "tsconfig-paths": "4.2.0",
    "typescript": "5.0.4"
  }
}
